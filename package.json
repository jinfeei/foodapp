{
  "name": "foodapp",
  "version": "1.0.0",
  "scripts": {
    "start": "ts-node --transpile-only ./src/server.ts",
    "devx": "nodemon --watch './src/**/*.ts' --exec ts-node ./src/server.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec node --inspect=0.0.0.0:9229 -r ts-node/register ./src/server.ts",
    "build": "tsc",
    "test": "jest --watchAll"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "fastify": "^3.3.0",
    "fastify-mysql": "^2.0.1",
    "fastify-static": "^3.2.0",
    "firebase-admin": "^9.1.1",
    "middie": "^5.1.0",
    "pino-pretty": "^4.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.3",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "nodemon": "^1.19.0"
  },
  "description": "FoodApp node.js server",
  "main": "jest.config.js",
  "directories": {
    "test": "test"
  },
  "author": "Loh Jin Feei <jinfeei.loh@gmail.com>",
  "license": "ISC"
}
